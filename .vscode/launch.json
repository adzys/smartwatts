{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        
      {
        "name": "Attach to Chrome",
        "port": 9222,
        "request": "attach",
        "type": "chrome",
        "webRoot": "${workspaceFolder}"
      },
      {
        "name": "Flutter Web",
        "request": "launch",
        "type": "dart",
        "deviceId": "chrome",
        "program": "lib/main.dart"
      },
      {
        "name": "smartwatts",
        "request": "launch",
        "type": "dart"
      },
      {
        "name": "smartwatts (debug mode)",
        "request": "launch",
        "flutterMode": "debug",
        "args": ["--no-sound-null-safety"],
        "type": "dart"
      },
      {
        "name": "smartwatts (profile mode)",
        "request": "launch",
        "type": "dart",
        "flutterMode": "profile"
      },
      {
        "name": "smartwatts (release mode)",
        "request": "launch",
        "type": "dart",
        "flutterMode": "release"
      },
      {
        "name": "smartwatts",
        "cwd": "smartwatts",
        "request": "launch",
        "type": "dart"
      },
      {
        "name": "smartwatts (profile mode)",
        "cwd": "smartwatts",
        "request": "launch",
        "type": "dart",
        "flutterMode": "profile"
      },
      {
        "name": "smartwatts (release mode)",
        "cwd": "smartwatts",
        "request": "launch",
        "type": "dart",
        "flutterMode": "release"
      }
    ]
  }  